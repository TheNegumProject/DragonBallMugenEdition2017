[Output]
 ;Filename of the SFF file to create (required).
filename = font/num72.sff

[Option]
 ;Directory containing source files.
;input.dir =
 ;Use compression for medium and large fonts.
sprite.compress.5 = lz5
sprite.compress.8 = rle8
sprite.compress.24 = png24
sprite.compress.32 = png32
 ;Decompressed when loaded from disk.
sprite.decompressonload = 0
 ;0 - Detect identical files only through filename.
sprite.detectduplicates = 0
 ;0 - Do not auto-crop sprites (default).
sprite.autocrop = 0
 ;0 - Detect identical files only through filename.
pal.detectduplicates = 0
 ;0 - Do not discard duplicate palettes (default).
pal.discardduplicates = 0

[Option]
sprite.usepal = -1

;Sprites
;group,itemno, fname, axisx, axisy
[Sprite]
;For font sprites, group,itemno should be 0,ASCII-code (See reference at bottom).
;Sorry, no UTF-8 support!
; Bank 0 - yellow
0, 48, num72.png,  0, 0           ? crop=0,0,69,70
0, 49, num72.png, 70, 0           ? crop=70,0,139,70
0, 50, num72.png,140, 0           ? crop=140,0,209,70
0, 51, num72.png,210, 0           ? crop=210,0,279,70
0, 52, num72.png,280, 0           ? crop=280,0,349,70
0, 53, num72.png,350, 0           ? crop=350,0,419,70
0, 54, num72.png,420, 0           ? crop=420,0,489,70
0, 55, num72.png,490, 0           ? crop=490,0,559,70
0, 56, num72.png,560, 0           ? crop=560,0,629,70
0, 57, num72.png,630, 0           ? crop=630,0,699,70
0, 111,num72.png,700, 0           ? crop=700,0,783,70
